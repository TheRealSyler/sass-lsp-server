{
  "name": "sass-lsp-server",
  "version": "1.0.0",
  "author": "Syler",
  "license": "MIT",
  "private": true,
  "main": "./dist/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheRealSyler/sass-lsp-server"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "yarn build",
    "build": "del ./dist && tsc -b",
    "watch": "tsc -w",
    "test": "jest --coverage",
    "test:ci": "jest --coverage --ci -w 1",
    "test:watch": "jest --clearCache && jest --watch --coverage"
  },
  "dependencies": {
    "suf-regex": "^0.0.18",
    "vscode-languageserver": "^6.1.1",
    "vscode-languageserver-textdocument": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "del-cli": "^3.0.0",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
